{
  "name": "nginx-plus-dashboard",
  "version": "3.0.2",
  "description": "Nginx+ dashboard",
  "main": "index.js",
  "scripts": {
    "clean-dist": "rm -rf ./dist",
    "build": "NODE_ENV=production FILE_NAME=dashboard webpack",
    "build-demo": "NODE_ENV=production FILE_NAME=demo DASHBOARD_TYPE=demo webpack",
    "build-all": "yarn clean-dist && yarn build && yarn build-demo",
    "scan": "yarn audit --groups dependencies",
    "scan-dev": "yarn audit --groups devDependencies",
    "start-dev": "yarn clean-dist && NODE_ENV=development FILE_NAME=dashboard_dev webpack-dev-server",
    "start-demo": "yarn clean-dist && NODE_ENV=development FILE_NAME=demo_dev DASHBOARD_TYPE=demo webpack-dev-server",
    "start-prod": "yarn clean-dist && NODE_ENV=production FILE_NAME=dashboard webpack-dev-server",
    "start-demo-prod": "yarn clean-dist && NODE_ENV=production FILE_NAME=demo DASHBOARD_TYPE=demo webpack-dev-server",
    "test": "karma start",
    "test-demo": "DASHBOARD_TYPE=demo karma start"
  },
  "author": "Ivan Poluyanov <ip@nginx.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-istanbul": "^6.1.1",
    "core-js": "^3.19.2",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.12",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-webpack-plugin": "^3.1.1",
    "history": "^4.7.2",
    "html-inline-css-webpack-plugin": "^1.11.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-font-open-sans": "^1.1.0",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "postcss-url": "^10.1.3",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "preact-portal": "^1.1.2",
    "react-dev-utils": "^12.0.0-next.58",
    "regenerator-runtime": "^0.13.9",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.2",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-mocha": "2.0.1",
    "karma-sinon-chai": "2.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.0.1",
    "nyc": "15.1.0",
    "preact-compat-enzyme": "^0.2.5",
    "preact-render-to-string": "^3.6.3",
    "preact-test-utils": "^0.1.3",
    "sinon": "9.0.2",
    "sinon-chai": "^3.5.0",
    "webpack-dev-server": "^4.6.0"
  }
}
