{
  "name": "angie-dashboard",
  "version": "3.2.0",
  "description": "Angie dashboard",
  "main": "index.js",
  "scripts": {
    "clean-dist": "rm -rf ./dist",
    "build": "NODE_ENV=production FILE_NAME=dashboard webpack",
    "build-demo": "NODE_ENV=production FILE_NAME=demo DASHBOARD_TYPE=demo webpack",
    "build-all": "yarn clean-dist && yarn build && yarn build-demo",
    "scan": "yarn audit --groups dependencies",
    "scan-dev": "yarn audit --groups devDependencies",
    "start-dev": "yarn clean-dist && NODE_ENV=development FILE_NAME=dashboard_dev webpack-dev-server",
    "start-demo": "yarn clean-dist && NODE_ENV=development FILE_NAME=demo_dev DASHBOARD_TYPE=demo webpack-dev-server",
    "start-prod": "yarn clean-dist && NODE_ENV=production FILE_NAME=dashboard webpack-dev-server",
    "start-demo-prod": "yarn clean-dist && NODE_ENV=production FILE_NAME=demo DASHBOARD_TYPE=demo webpack-dev-server",
    "test": "karma start",
    "test-demo": "DASHBOARD_TYPE=demo karma start"
  },
  "author": "Ivan Poluyanov <ip@nginx.com>",
  "license": "Apache-2.0",
  "overrides": {
    "minimist": "1.2.6",
    "cheerio": "1.0.0-rc.10",
    "loader-utils": "3.2.1"
  },
  "dependencies": {
    "@babel/core": "7.18.2",
    "@babel/plugin-proposal-object-rest-spread": "7.18.0",
    "@babel/plugin-transform-runtime": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.3.0",
    "babel-plugin-istanbul": "6.1.1",
    "core-js": "3.22.8",
    "css-loader": "6.7.1",
    "cssnano": "5.1.11",
    "eslint": "8.47.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-webpack-plugin": "4.0.1",
    "history": "4.10.1",
    "html-inline-css-webpack-plugin": "1.11.1",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.0",
    "npm-font-open-sans": "1.1.0",
    "postcss": "8.4.14",
    "postcss-loader": "6.2.1",
    "postcss-url": "10.1.3",
    "preact": "10.7.3",
    "react-dev-utils": "12.0.1",
    "regenerator-runtime": "0.13.9",
    "style-loader": "3.3.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "4.0.1",
    "karma": "6.3.20",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.2",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-mocha": "2.0.1",
    "karma-sinon-chai": "2.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "5.0.0",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "sinon": "9.0.2",
    "sinon-chai": "3.7.0",
    "webpack-dev-server": "4.9.2"
  }
}
