{
  "name": "angie-console-light",
  "version": "1.8.1",
  "description": "Angie Console Light",
  "main": "index.js",
  "scripts": {
    "clean-dist": "rm -rf ./dist",
    "build": "NODE_ENV=production FILE_NAME=console webpack",
    "build-demo": "NODE_ENV=production FILE_NAME=demo CONSOLE_TYPE=demo webpack",
    "build-all": "yarn clean-dist && yarn build && yarn build-demo",
    "scan": "yarn audit --groups dependencies",
    "scan-dev": "yarn audit --groups devDependencies",
    "start-dev": "yarn clean-dist && NODE_ENV=development FILE_NAME=console_dev webpack-dev-server",
    "start-demo": "yarn clean-dist && NODE_ENV=development FILE_NAME=demo_dev CONSOLE_TYPE=demo webpack-dev-server",
    "start-prod": "yarn clean-dist && NODE_ENV=production FILE_NAME=console webpack-dev-server",
    "start-demo-prod": "yarn clean-dist && NODE_ENV=production FILE_NAME=demo CONSOLE_TYPE=demo webpack-dev-server",
    "test": "jest",
    "test-demo": "CONSOLE_TYPE=demo jest",
    "spellcheck": "npx cspell-cli@8.19.1 -c cspell.config.json"
  },
  "author": "Ivan Poluyanov <ip@nginx.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=19 <22"
  },
  "resolutions": {
    "minimist": "1.2.6",
    "cheerio": "1.0.0-rc.10"
  },
  "dependencies": {
    "@babel/core": "7.18.2",
    "@babel/plugin-proposal-object-rest-spread": "7.18.0",
    "@babel/plugin-transform-runtime": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.3.0",
    "babel-plugin-istanbul": "6.1.1",
    "clipboard": "^2.0.11",
    "core-js": "3.22.8",
    "css-loader": "6.7.1",
    "cssnano": "5.1.11",
    "eslint": "8.47.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-webpack-plugin": "4.0.1",
    "file-loader": "^6.2.0",
    "history": "4.10.1",
    "html-inline-css-webpack-plugin": "1.11.1",
    "html-webpack-plugin": "5.5.0",
    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "mini-css-extract-plugin": "2.6.0",
    "monaco-editor": "0.41.0",
    "npm-font-open-sans": "1.1.0",
    "postcss": "8.4.14",
    "postcss-loader": "6.2.1",
    "postcss-url": "10.1.3",
    "preact": "10.7.3",
    "react-collapsible": "^2.10.0",
    "react-dev-utils": "12.0.1",
    "react-i18next": "^9.0.10",
    "regenerator-runtime": "0.13.9",
    "style-loader": "3.3.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@cspell/dict-ru_ru": "^2.2.4",
    "@types/jest": "^29.5.4",
    "babel-jest": "^29.6.4",
    "copy-webpack-plugin": "6.3.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.4",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.6.4",
